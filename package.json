{
  "scripts": {
    "install": "cd react && yarn install",
    "start": "cd react && yarn start",
    "build": "cd react && yarn build",
    "build-mv": "rm -rf src/main/resources/build && yarn build && mv react/build src/main/resources/build && yarn mv-lib",
    "build-move": "rmdir /s /q src\\main\\resources\\build && yarn build && move react\\build src\\main\\resources\\build",
    "mv": "rm -rf src/main/resources/build && mv react/build src/main/resources/build",
    "move": "rmdir /s /q src\\main\\resources\\build && move react\\build src\\main\\resources\\build",
    "mv-lib": "cp src/main/resources/html/css/material-dashboard.css src/main/resources/build/static/css/material-dashboard.css",
    "clean": "cd react && yarn clean",
    "lint": "cd react && yarn lint"
  },
  "dependencies": {
    "highcharts-more": "^0.1.7"
  }
}
